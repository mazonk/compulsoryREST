{
    "Logging": {
      "LogLevel": {
        "Default": "process.env.LOGGING__DEFAULT || Debug",
        "Microsoft.AspNetCore": "process.env.LOGGING__MICROSOFT_ASPNETCORE || Debug"
      }
    },
    "AllowedHosts": "*",
    "MongoDB": {
      "ConnectionURI": "process.env.MONGODB_CONNECTION_URI",
      "DatabaseName": "process.env.MONGODB_DATABASE_NAME",
      "CollectionName": "process.env.MONGODB_COLLECTION_NAME"
    }
  }